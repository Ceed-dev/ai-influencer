{
  "name": "ai-influencer-v5",
  "version": "5.0.0",
  "description": "AI Influencer v5.0 - Multi-agent content production system",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:unit": "jest --testPathPattern='tests/unit'",
    "test:integration": "jest --testPathPattern='tests/integration'",
    "test:smoke": "jest --testPathPattern='tests/smoke'",
    "test:e2e": "jest --testPathPattern='tests/e2e'",
    "lint": "eslint 'src/**/*.ts' 'types/**/*.ts'",
    "typecheck": "tsc --noEmit",
    "db:migrate": "drizzle-kit migrate",
    "db:seed": "tsx src/db/seed.ts"
  },
  "dependencies": {
    "@langchain/langgraph": "0.2.19",
    "@langchain/anthropic": "0.3.14",
    "@langchain/core": "0.3.30",
    "@modelcontextprotocol/sdk": "1.7.0",
    "drizzle-orm": "0.36.4",
    "pg": "8.13.1",
    "@fal-ai/client": "1.9.3",
    "next": "14.2.18",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "@monaco-editor/react": "4.6.0",
    "tailwindcss": "3.4.17",
    "recharts": "2.12.7",
    "@tremor/react": "3.17.4",
    "dotenv": "16.4.7",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "typescript": "5.7.3",
    "drizzle-kit": "0.30.1",
    "jest": "29.7.0",
    "ts-jest": "29.2.5",
    "@types/jest": "29.5.14",
    "@types/node": "20.17.12",
    "@types/pg": "8.11.10",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "tsx": "4.19.2",
    "eslint": "9.18.0",
    "@eslint/js": "9.18.0",
    "typescript-eslint": "8.21.0",
    "prettier": "3.4.2"
  }
}
